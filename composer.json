  "authors": [{
    "name": "Cully Larson",
    "email": "cully.larson@gmail.com"
  }],
  "require": {
    "php": ">=5.3.0",
    "cullylarson/ssh-commands": "~1.0",
    "cullylarson/local-commands": "~1.0",
    "interconnectit/search-replace-db": "~3.1"
  },
  "require-dev": {
